= true\r\n        super.preUpdate(t, dt)\r\n \r\n        const speed = 50\r\n\r\n        switch (this.direction)\r\n        {\r\n            case 0: //up\r\n                this.setVelocity(0, -speed)\r\n                break\r\n            \r\n            case 1: //down\r\n                this.setVelocity(0, speed)\r\n                break\r\n        \r\n            case 2: //left\r\n                this.setVelocity(-speed, 0)\r\n                break\r\n    \r\n            case 3: //right\r\n                this.setVelocity(speed, 0)\r\n                break\r\n                \r\n        }\r\n\r\n    }\r\n\r\n  \r\n}"},"lineCount":null}},"error":null,"hash":"e92424b94d98a715fdf8dbe0e516189f","cacheData":{"env":{}}}